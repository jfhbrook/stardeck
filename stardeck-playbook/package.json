    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^15.10.0",
    "prettier": "^3.0.0",
    "vitest": "^2.1.2"
