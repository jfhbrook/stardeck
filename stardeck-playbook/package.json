{
  "name": "@jfhbrook/stardeck-playbook",
  "version": "1.0.0",
  "description": "Ansible playbook runner for Stardeck",
  "type": "module",
  "main": "index.mjs",
  "bin": {
    "stardeck-playbook": "main.mjs"
  },
  "scripts": {
    "format": "prettier --write .",
    "start": "node ./main.mjs",
    "lint": "prettier --list-different . && eslint --fix \"**.mjs\"",
